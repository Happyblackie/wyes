/*** Footer ***/
.footer-sticky{
  position: fixed;
  bottom: 0;
  width: 100%;
  z-index: 98;
}

.footer{
 margin-top:0 !important;
}

.footer P{

  font-size: 12px !important;
  font-weight: 400;
  color:#fff;
}

.footer .btn.btn-link li a{
  display: block;
  margin-bottom: 5px;
  padding: 0;
  text-align: left;
  color: var(--light);
  text-transform: capitalize;
  transition: .3s;
  font-size: 12px !important;
  font-weight: 400;
 
}

.footer .btn.btn-link li a::before {
  position: relative;
  content: "\f105";
  font-family: "Font Awesome 5 Free";
  font-weight: 900;
  margin-right: 10px;
}

.footer .btn.btn-link li a:hover {
  color: var(--secondary);
  letter-spacing: 1px;
  box-shadow: none;
}

.footer .btn-link {
  list-style-type: none;
}
.footer .btn {
 padding: 0;
}

.copyright {
  color: var(--light);
  background: #1b55a9;
  font-size: 12px !important;
  height: 48px;

}

.copyright a {
  color: var(--light);
}

.copyright a:hover {
  color: var(--primary);
}

.owl-nav.disabled {
  display: block !important;
}

.text-mini{
  color: #da0d15 !important;
}

.service p{
  margin-bottom: 0 !important;
}

