/********** Template CSS **********/
body {
  margin: 0;
  font-family: 'Open Sans', sans-serif !important;
  font-size: 1rem;
  font-weight: 100 !important;
  line-height: 1.5;
  color: #222;
  background-color: #fff;
  -webkit-text-size-adjust: 100%;
  -webkit-tap-highlight-color: rgba(0,0,0,0);
}

:root {
  --primary: #011A41;
  --secondary: #ea0d2c;
  --tertiary: #555555;
  --light: #DFE4FD;
  --dark: #ea0d2c;
}

.back-to-top {
  position: fixed;
  display: none;
  right: 30px;
  bottom: 30px;
  z-index: 99;
}

h1,
.h1,
h2,
.h2,
.fw-bold {
  font-weight: 400 !important;
  font-family: Georgia,Times,Times New Roman,serif; 
  font-size: 38px !important;
}

h3,
.h3,
h4,
.h4,
.fw-medium {
  font-weight: 400 !important;
  font-family: Georgia,Times,Times New Roman,serif; 
}

h5,
.h5,
h6,
.h6,
.fw-semi-bold {
  font-weight: 400 !important;
  font-family: Georgia,Times,Times New Roman,serif; 
}


/*** Spinner ***/
#spinner {
  opacity: 0;
  visibility: hidden;
  transition: opacity .5s ease-out, visibility 0s linear .5s;
  z-index: 99999;
}

#spinner.show {
  transition: opacity .5s ease-out, visibility 0s linear 0s;
  visibility: visible;
  opacity: 1;
}


/*** Button ***/
.btn {
  transition: .5s;
  font-weight: 500;
}

.btn-primary,
.btn-outline-primary:hover {
  color: var(--light);
}

.btn-square {
  width: 38px;
  height: 38px;
}

.btn-sm-square {
  width: 32px;
  height: 32px;
}

.btn-lg-square {
  width: 48px;
  height: 48px;
}

.btn-square,
.btn-sm-square,
.btn-lg-square {
  padding: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: normal;
  z-index: 301;
}


/*** Navbar ***/
.fixed-top {
  transition: .5s;
}

.top-bar {
  height: 26px;
  border-bottom: 1px solid rgba(218, 13, 21, .8);
  background-color: #e3ebf6;
}

.navbar {
  background-color: #fff;
}

.navbar .dropdown-toggle::after {
  border: none;
  content: "\f107";
  font-family: "Font Awesome 5 Free";
  font-weight: 900;
  vertical-align: middle;
  margin-left: 8px;
}

.navbar .navbar-nav .nav-link {
  padding: 19px 15px;
  color:var(--tertiary);
  font-weight: 400;
  outline: none;
}

.navbar-expand-lg .navbar-collapse {
 
  flex-basis: 100% !important;
}

.navbar .navbar-nav .nav-link:hover,
.navbar .navbar-nav .nav-link.active {
  color: var(--secondary);
  padding-top: 32px;
}

/*Sino soft Navbar */
.navbar {
  background-color: #fff;
}

.navbar .dropdown-toggle::after {
  border: none;
  content: "\f107";
  font-family: "Font Awesome 5 Free";
  font-weight: 900;
  vertical-align: middle;
  margin-left: 8px;
}

.navbar-nav li a {
  padding: 19px 15px;
  color:var(--tertiary);
  font-weight: 400;
  outline: none;
}

.navbar-expand-lg .navbar-collapse {
 
  flex-basis: 100% !important;
}

.navbar-nav li a:hover,
.navbar-nav li .active {
  color: var(--secondary) !important;
}

.current-menu-item a {
  color: var(--secondary) !important;

}




@media (max-width: 991.98px) {
  .navbar .navbar-nav {
      margin-top: 10px;
      border-top: 1px solid rgba(0, 0, 0, .07);
      background: #FFFFFF;
  }

  .navbar-nav li {
    padding: 10px 0;
}
}


@media (min-width: 992px) {
  .navbar .nav-item .dropdown-menu {
      display: block;
      visibility: hidden;
      top: 100%;
      transform: rotateX(-75deg);
      transform-origin: 0% 0%;
      transition: .5s;
      opacity: 0;
  }

  .navbar .nav-item:hover .dropdown-menu {
      transform: rotateX(0deg);
      visibility: visible;
      transition: .5s;
      opacity: 1;
  }
}

/*** Header ***/
.carousel-caption {
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: start;
  /* background: linear-gradient(to right, rgba(2, 36, 91, 1) 0%, rgba(2, 36, 91, 0) 100%); */
  /* background: linear-gradient(to right, rgba(218, 13, 21, 0.6) 0%, rgba(39, 48, 89, 1) 30%, rgba(218, 13, 21, 0) 100%); */
  background: linear-gradient(to right, rgba(39, 48, 89,  1) 3%, rgba(218, 13, 21, 0.5) 50%, rgba(218, 13, 21, 0) 100%);
  z-index: 1;
}
